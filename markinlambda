exports.handler = async (event) => {
    
    
    
    var recebido = JSON.parse(event.body);
    
    
    var resposta = {
        "informações": recebido.login,
        "mensagem": "logado com sucesso"
    }
    
    const response = {
        statusCode: 200,
        body: JSON.stringify(resposta)
    }
    return response;
};
